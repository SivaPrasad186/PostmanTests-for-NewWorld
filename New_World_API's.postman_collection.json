					"name": "Get All App Suites with apps",
					"event": [
						{
							"listen": "test",
							"script": {
								"id": "b56468fa-3145-4b4d-919c-c200a307ea0b",
								"exec": [
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test(\"Data is present\", function(){",
									"    ",
									"    var result;",
									"",
									"    for(var loop =0; loop < pm.response.json().data.length; loop++)",
									"    {",
									"        if(pm.response.json().data[loop].id===\"egton:appcatalogue:appsuite:6a7fe960-08ba-4591-88f7-c39e4d3f19df\"){",
									"            ",
									"        function count(object, key, subKey) {",
									"            const noObject = o => !o || typeof o !== 'object';",
									"            function subCount(object) {",
									"                if (noObject(object)) return 0;",
									"                if (subKey in object) return 1;",
									"                return Object.values(object).reduce((s, o) => s + subCount(o), 0);",
									"            }",
									"            if (noObject(object)) return 0;",
									"            if (key in object) return subCount(object[key]);",
									"             return Object.values(object).reduce((s, o) => s + count(o, key, subKey), 0);",
									"        }",
									"         var data = pm.response.json().data[loop].relationships;",
									"            console.log(count(data, 'apps', 'id'));",
									"        ",
									"            result=true;",
									"            break;",
									"           ",
									"        }",
									"    }",
									"    pm.expect(true).to.eql(result);",
									"})"
								],
								"type": "text/javascript"
							}
						}
